<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>PasswordStore</servlet-name>
  	<servlet-class>com.ty.controller.UserController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>PasswordStore</servlet-name>
  	<url-pattern>/passwordStore</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>EmployeeDetails</servlet-name>
  	<servlet-class>com.ty.controller.EmployeeController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>EmployeeDetails</servlet-name>
  	<url-pattern>/employeeDetails</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>ValidateEmployee</servlet-name>
  	<servlet-class>com.ty.controller.ValidateEmployeeController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ValidateEmployee</servlet-name>
  	<url-pattern>/validateEmployee</url-pattern>
  </servlet-mapping>
</web-app>
